{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "link-block/link-block",
  "version": "0.1.0",
  "title": "Link-block",
  "category": "widgets",
  "icon": "links",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "Icon": {
      "type": "string",
